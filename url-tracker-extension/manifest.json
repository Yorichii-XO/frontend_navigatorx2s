{
  "manifest_version": 3,
  "name": "URL Tracker Extension",
  "version": "2.0",
  "permissions": [
      "tabs",
      "activeTab",
      "storage"
  ],
  "host_permissions": [
      "<all_urls>"
  ],
  "background": {
      "service_worker": "background.js"
  },
  "action": {
      "default_popup": "popup.html"
  }
}
